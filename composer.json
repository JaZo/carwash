{
    "name": "dansoppelsa/carwash",
    "description": "Data scrubber for Laravel applications",
    "type": "library",
    "require": {
        "fzaninotto/faker": "^1.7",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.5",
        "orchestra/testbench" : "^4.0|^5.0",
        "orchestra/database": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4" : {
            "Carwash\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Carwash\\" : "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Carwash\\CarwashProvider"
            ]
        }
    }
}
